<p id="notice"><%= notice %></p>
<h1>Messages</h1>
<table>
  <thead>
    <tr>
      <!-- <th>User</th> -->
      <th>Message</th>
      <th>Updated at</th>
    </tr>
  </thead>
  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <!-- <td><%# notification.user_id %></td> -->
        <td><%= notification.message.html_safe %></td>
        <td><%= notification.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>